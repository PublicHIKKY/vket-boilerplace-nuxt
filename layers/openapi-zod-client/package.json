{
  "name": "vket-boilerplace-nuxt-openapi-zod-client",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "build": "bun build-pre && tsx ./tasks/openapi/make-zod.mts && bun build-post",
    "build-pre": "git submodule update --init --recursive",
    "build-post": "tsx ./tasks/openapi/replace.mts && bun fix:prettier",
    "typecheck": "echo 'TODO: Implement `bun typecheck`'",
    "lint": "bun lint:eslint",
    "lint:eslint": "eslint --cache --cache-strategy content ./tasks",
    "fix": "bun fix:eslint",
    "fix:eslint": "eslint --cache --cache-strategy content --fix ./tasks",
    "test:ut": "vitest run --dir ./app/test",
    "test:watch": "vitest --dir ./app/test",
    "test:ui": "vitest --ui --dir ./app/test",
    "test:coverage": "vitest run --dir ./app/test --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@fast-check/vitest": "^0.1.4",
    "@playwright/test": "^1.49.0",
    "@redocly/cli": "^1.26.1",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/ui": "^2.1.8",
    "camelcase-keys": "^9.1.3",
    "fast-check": "^3.23.1",
    "jsdom": "^25.0.1",
    "openapi-zod-client": "^1.18.2",
    "snakecase-keys": "^8.0.1",
    "vite-plugin-eslint2": "^5.0.3",
    "vitest": "^2.1.8",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": "22.x"
  }
}
